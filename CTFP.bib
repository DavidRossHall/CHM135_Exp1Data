@article{Notario2012,
abstract = {Concentrations of O 3, NO, NO 2, NO x and oxidant (OX=O 3+NO 2) were investigated for the first time, in the southwest of the Iberian Peninsula, in the Seville metropolitan area, during 2004. This area experiences frequent photochemical pollution events, mainly in the warm season. Two different environments were analyzed and compared, an urban traffic station (Torneo) and a suburban station (Aljarafe). The concentration of OX (defined as O 3+NO 2) can be described in terms of the sum of a NO x-independent 'regional' contribution (i.e. the O 3 background), and a linearly NO x-dependent 'local' contribution. Monthly dependence of regional and local OX concentration variation at Torneo and Aljarafe stations were studied along with the annual variation of the daily mean NO x and OX. Maximum levels of OX{\textgreater}190$\mu$gm -3 where NO x{\textless}70$\mu$gm -3 at Aljarafe, and OX{\textless}170$\mu$gm -3 with NO x around 80-90$\mu$gm -3 can be expected in summer months and during daylight hours. Also during summer, but at night, more elevated NO x levels are needed in order to achieve maximum OX values (OX=130-150$\mu$gm -3 with NO x≥100$\mu$gm -3 at Aljarafe, OX=110-130$\mu$gm -3 with NO x≥100$\mu$gm -3 at Torneo). This can be used as an approximation to estimate the OX concentrations in Seville for each month, as a function of the NO x measured, as well as to improve in the atmospheric photochemical dynamic in the Spanish Western Mediterranean area where there are undeniable air quality problems. {\textcopyright} 2011 Elsevier B.V.},
author = {Notario, Alberto and Bravo, Iv{\'{a}}n and Adame, Jos{\'{e}} Antonio and D{\'{i}}az-de-Mera, Yolanda and Aranda, Alfonso and Rodr{\'{i}}guez, Ana and Rodr{\'{i}}guez, Diana},
doi = {10.1016/j.atmosres.2011.10.008},
file = {:C$\backslash$:/Users/David Hall/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Notario et al. - 2012 - Analysis of NO, NO 2, NO x, O 3 and oxidant (OX=O 3NO 2) levels measured in a metropolitan area in the southwest.pdf:pdf},
issn = {01698095},
journal = {Atmospheric Research},
keywords = {Air quality,Local pollution,Nitrogen oxides,Oxidant,Ozone,Regional pollution,Urban pollution},
month = {feb},
pages = {217--226},
publisher = {Elsevier},
title = {{Analysis of NO, NO 2, NO x, O 3 and oxidant (OX=O 3+NO 2) levels measured in a metropolitan area in the southwest of Iberian Peninsula}},
volume = {104-105},
year = {2012}
}
