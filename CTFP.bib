@article{Galloway2015,
abstract = {Research on learning in the undergraduate chemistry laboratory necessitates an understanding of students' perspectives of learning. Novak's Theory of Meaningful Learning states that the cognitive (thinking), affective (feeling), and psychomotor (doing) domains must be integrated for meaningful learning to occur. The psychomotor domain is the essence of the chemistry laboratory, but the extent to which the cognitive and affective domains are integrated into the laboratory is unknown. For meaningful learning to occur in the undergraduate chemistry laboratory, students must actively integrate both the cognitive domain and the affective domains into the "doing" of their laboratory work. The Meaningful Learning in the Laboratory Instrument (MLLI) was designed to measure students' expectations before and after laboratory courses and experiences, in both the cognitive and affective domains, within the context of conducting experiments in the undergraduate chemistry laboratory. The MLLI was pilot-tested and modified based on an analysis of the pilot study data. The revised, 31-item MLLI was administered online both at the beginning and end of a semester to both general and organic chemistry laboratory students. Evidence for both the validity and reliability of the data, as well as comparisons between general and organic chemistry students' responses, are discussed.},
author = {Galloway, Kelli R. and Bretz, Stacey Lowery},
doi = {10.1021/ed500881y},
file = {:C$\backslash$:/Users/David Hall/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Galloway, Bretz - 2015 - Development of an Assessment Tool to Measure Students' Meaningful Learning in the Undergraduate Chemistry Labor.pdf:pdf},
issn = {19381328},
journal = {Journal of Chemical Education},
keywords = {Assessment,Chemistry Education Research,First-Year Undergraduate,Hands-On Learning,Laboratory Instruction,Learning Theories,Organic Chemistry,Second-Year Undergraduate},
month = {mar},
number = {7},
pages = {1149--1158},
publisher = {American Chemical Society},
title = {{Development of an Assessment Tool to Measure Students' Meaningful Learning in the Undergraduate Chemistry Laboratory}},
volume = {92},
year = {2015}
}
